${|title|}maked:
ifeq ($(UNAME), win32)
	.\node_modules\.bin\webpack-dev-server --config .\webpack.dev.js
else
	./node_modules/.bin/webpack-dev-server --config ./webpack.dev.js
endif

tests:
ifeq ($(UNAME), win32)
	.\node_modules\.bin\webpack --config .\webpack.config.js
else
	./node_modules/.bin/webpack --config ./webpack.config.js
endif

build:
ifeq ($(UNAME), win32)
	.\node_modules\.bin\webpack --config .\webpack.config.js
else
	./node_modules/.bin/webpack --config ./webpack.config.js
endif
